{# templates/feed/baagl/missing.html.twig #}
{% extends 'base.html.twig' %}{% block title %}Chybějící podklady{% endblock %}
{% block body %}
  <h2>Chybějící (bez kategorie nebo ceny)</h2>
  <p>Počet: {{ rows|length }}</p>
  <table border="1" cellpadding="6">
    <tr><th>ID</th><th>SKU</th><th>Název</th><th>EAN</th><th>Size</th><th>PC€</th><th>PC Kč</th><th>Category src</th></tr>
    {% for r in rows %}
      <tr>
        <td>{{ r.id }}</td><td>{{ r.artikl }}</td><td>{{ r.name }}</td><td>{{ r.ean }}</td>
        <td>{{ r.size_label }}</td><td>{{ r.pc_eur }}</td><td>{{ r.pc_czk_vat }}</td><td>{{ r.category_src }}</td>
      </tr>
    {% endfor %}
  </table>
{% endblock %}
